{
  "name": "maze-example",
  "version": "0.0.1",
  "description": "",
  "author": {
    "name": "ytoune"
  },
  "default": "dist/index.html",
  "browserslist": [
    "> 1.0%",
    "not op_mini all"
  ],
  "targets": {
    "default": {
      "publicUrl": "/maze-example/"
    }
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "deploy": "yarn build && yarn push-page",
    "push-page": "gh-pages -d dist",
    "build": "ts-node scripts/prebuild.ts && parcel build src/index.html --no-cache --no-source-maps",
    "watch": "parcel watch src/index.html",
    "start": "parcel src/index.html --open",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/preset-typescript": "7.12.7",
    "@types/fs-extra": "9.0.4",
    "@types/gh-pages": "3.0.0",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.10",
    "@typescript-eslint/eslint-plugin": "4.8.2",
    "@typescript-eslint/parser": "4.8.2",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.1.4",
    "fs-extra": "9.0.1",
    "gh-pages": "3.1.0",
    "jest": "26.6.3",
    "parcel": "1.12.4",
    "prettier": "2.2.0",
    "rxjs": "6.6.3",
    "sass": "1.29.0",
    "sass-loader": "10.1.0",
    "ts-jest": "26.4.4",
    "ts-node": "9.0.0",
    "typescript": "4.1.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "ts-jest"
    }
  },
  "resolutions": {
    "node-forge": "0.10.0"
  }
}
