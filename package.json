{
  "name": "maze-example",
  "version": "0.0.1",
  "description": "",
  "author": {
    "name": "ytoune"
  },
  "default": "dist/index.html",
  "targets": {
    "default": {
      "publicUrl": "/maze-example/",
      "engines": {
        "browsers": [
          "> 1.0%",
          "not op_mini all"
        ]
      }
    }
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "deploy": "yarn build && yarn push-page",
    "push-page": "gh-pages -d dist",
    "build": "ts-node scripts/prebuild.ts && parcel build src/index.html --no-cache --no-source-maps",
    "watch": "parcel watch src/index.html",
    "start": "parcel src/index.html --open",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/preset-typescript": "7.7.7",
    "@types/fs-extra": "8.0.1",
    "@types/gh-pages": "2.0.1",
    "@types/jest": "24.0.25",
    "@types/node": "13.1.5",
    "@typescript-eslint/eslint-plugin": "2.15.0",
    "@typescript-eslint/parser": "2.15.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-prettier": "3.1.2",
    "fs-extra": "8.1.0",
    "gh-pages": "2.2.0",
    "jest": "24.9.0",
    "parcel": "2.0.0-alpha.3.2",
    "prettier": "1.19.1",
    "rxjs": "6.5.4",
    "sass": "1.24.3",
    "sass-loader": "8.0.0",
    "ts-jest": "24.3.0",
    "ts-node": "8.5.4",
    "typescript": "3.7.4"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "ts-jest"
    }
  }
}
