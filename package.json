{
  "name": "maze-example",
  "version": "0.0.1",
  "description": "",
  "author": {
    "name": "ytoune"
  },
  "default": "dist/index.html",
  "browserslist": [
    "> 1.0%",
    "not op_mini all"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "deploy": "yarn build && yarn push-page",
    "push-page": "gh-pages -d dist",
    "build": "rimraf dist && parcel build src/index.html --no-cache --no-source-maps --public-url /maze-example/",
    "build-local": "rimraf dist && parcel build src/index.html --no-cache --no-source-maps --public-url /",
    "watch": "parcel watch src/index.html",
    "start": "parcel src/index.html --open",
    "test": "eslint '**/*.ts' --ignore-path .gitignore && tsc --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.14.0",
    "@babel/preset-typescript": "7.13.0",
    "@parcel/transformer-sass": "2.0.0-beta.2",
    "@types/gh-pages": "3.0.0",
    "@types/jest": "26.0.23",
    "@types/node": "15.0.2",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "eslint": "7.26.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.4.0",
    "gh-pages": "3.1.0",
    "jest": "26.6.3",
    "parcel": "2.0.0-beta.2",
    "prettier": "2.3.0",
    "rimraf": "3.0.2",
    "rxjs": "7.0.0",
    "sass": "1.32.12",
    "sass-loader": "11.1.0",
    "ts-jest": "26.5.6",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "ts-jest"
    }
  },
  "resolutions": {
    "node-forge": "0.10.0"
  }
}
